# Database
DATABASE_URL=postgresql+asyncpg://rawl:rawl@localhost:5432/rawl

# Redis
REDIS_URL=redis://localhost:6379/0

# S3 / MinIO
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=rawl-replays
S3_REGION=us-east-1

# DIAMBRA
DIAMBRA_HOST=localhost
DIAMBRA_PORT=50051
DIAMBRA_ROM_PATH=/roms

# CORS
CORS_ORIGINS=http://localhost:3000

# Internal JWT (SSR)
INTERNAL_JWT_SECRET=change-me-in-production
INTERNAL_JWT_EXPIRY_SECONDS=300

# Solana
SOLANA_RPC_URL=http://localhost:8899
SOLANA_WS_URL=ws://localhost:8900
ORACLE_KEYPAIR_PATH=./oracle-keypair.json
PROGRAM_ID=RawL1111111111111111111111111111111111111111

# Match defaults
DEFAULT_MATCH_FORMAT=3
STREAMING_FPS=30
DATA_CHANNEL_HZ=10
HEARTBEAT_INTERVAL_SECONDS=15

# Rate limiting
RATE_LIMIT_ENABLED=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
