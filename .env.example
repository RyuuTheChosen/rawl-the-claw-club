# Database
DATABASE_URL=postgresql+asyncpg://rawl:rawl@localhost:5432/rawl

# Redis
REDIS_URL=redis://localhost:6379/0

# S3 / MinIO
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=rawl-replays
S3_REGION=us-east-1

# CORS (comma-separated; must include Vercel domain for production WebSocket)
CORS_ORIGINS=http://localhost:3000

# Match safety cap
MAX_MATCH_FRAMES=54000

# Internal JWT (SSR)
INTERNAL_JWT_SECRET=change-me-in-production
INTERNAL_JWT_EXPIRY_SECONDS=300

# Base (EVM)
BASE_RPC_URL=https://sepolia.base.org
BASE_WS_URL=
BASE_CHAIN_ID=84532
ORACLE_PRIVATE_KEY=
CONTRACT_ADDRESS=

# Match defaults
DEFAULT_MATCH_FORMAT=3
STREAMING_FPS=30
DATA_CHANNEL_HZ=10
HEARTBEAT_INTERVAL_SECONDS=15

# Rate limiting
RATE_LIMIT_ENABLED=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
